#include<stdio.h> 
int stack[100],n,top,x,i; // static varables//
void push(void) // pushing elements into array//
{ 
     printf(" Enter a value to be pushed:"); 
     scanf("%d",&x); 
     top++; 
     stack[top]=x; 
 } 
void pop(void) // popping elements from array//
{ 
  printf(" The removed elements is %d \n",stack[top]); 
        top--; 
} 
void display(void) // diaplay the elements//
{ 
      if(top>=0) 
    { 
       printf(" The elements in stack \n"); 
       for(i=top; i>=0; i--) 
        printf("%d\n",stack[i]); 
    } 
    else 
    { 
        printf(" The stack is empty \n"); 
    } 
} 
int main() 
{ 
  top=-1; 
    push(); 
    push(); 
    push(); 
    push(); 
    push(); 
    display(); 
    pop(); 
    pop(); 
    printf(" elements in stack after removing \n"); 
    display(); 
}
